create or replace procedure P_cory_test(tablename varchar2)
is
num number;
begin
    select count(1) into num from user_tables where table_name = upper(tablename || '_test') ;
    if num > 0 then
        execute immediate 'drop table '||tablename||'_test' ;
    end if;
    execute immediate 'create table '||tablename||'_test as select * from '||tablename;
end P_cory_test;

create or replace procedure model.P_getDept
(p_month varchar2, p_tablename varchar2
) is
begin
  execute immediate 'update '||p_tablename||' t set t.deductedept = ''皮肤科'', t.deducteproject = ''项目核算科室'' where t.month = '''||p_month||'''';
  commit;
end P_getDept;


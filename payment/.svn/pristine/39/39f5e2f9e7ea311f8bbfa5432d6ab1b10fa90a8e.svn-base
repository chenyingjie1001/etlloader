<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mllpservice.mapper.mllp.MllpMapper">

    <insert id="addMllp">
        insert into drgs.mllp_message(id,messagehappentime,messagereturntime,createdate,updatedate,operateor,delflag,message)
        VALUES (sys_guid(),#{messageHappenTime},#{messageReturnTime},sysdate,sysdate,'管理员','0',#{message})
    </insert>

</mapper>